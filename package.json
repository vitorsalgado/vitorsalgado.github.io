{
  "name": "vitorsalgado.github.io",
  "description": "My personal open source profile",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config build/webpack/webpack.dev.js",
    "start:headless": "cross-env NODE_ENV=production webpack serve --config build/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack build --config build/webpack/webpack.production.js",
    "test": "jest src --detectOpenHandles",
    "test:ci": "jest --detectOpenHandles --runInBand --coverage --ci",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint \"**/*.{css}\" --allow-empty-input",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:ci": "npm run lint && npm run lint:css && npm run prettier:ci",
    "prettier": "prettier 'build/**' 'config/**' 'src/**' --write --ignore-unknown",
    "prettier:ci": "npm run prettier --check",
    "prepare": "husky install"
  },
  "license": "MIT",
  "author": "vitorsalgado",
